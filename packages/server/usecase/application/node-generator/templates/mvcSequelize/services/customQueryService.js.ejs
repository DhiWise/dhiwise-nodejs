/** 
 * customQueryService.js
 * @description :: exports functions used in manipulating data from database
 */

const find = async (model,{
    filter = {}, attributes=[], order=[] 
}) => {
    const result = await model.findAll({
      where: filter
    },
    {attributes: attributes},
    {order: order})
    return result;
};

module.exports = {
    find,
}
