{
	"presets": [
		"@babel/preset-react",
		[
			"@babel/env",
			{
				"targets": {
					"edge": "17",
					"firefox": "60",
					"chrome": "67",
					"safari": "11.1"
				},
				"useBuiltIns": "usage",
				"corejs": "3.6.4"
			}
		]
	],
	"plugins": ["@babel/plugin-transform-react-jsx","@babel/plugin-proposal-class-properties"]
}